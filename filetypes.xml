<application>
  <component name="FileTypeManager" version="18">
    <ignoreFiles list="*$py.class;*.pyc;*.pyo;*.rbc;*.yarb;*~;.DS_Store;.git;.hg;.svn;CVS;__pycache__;_svn;vssver.scc;vssver2.scc" />
    <extensionMap>
      <mapping pattern="prettier" type="AUTO_DETECTED" />
      <mapping pattern="prepare-commit-msg" type="AUTO_DETECTED" />
      <mapping pattern="Jenkinsfile" type="Groovy" />
      <mapping ext="space.kts" type="Kotlin" />
      <mapping ext="gradle.kts" type="Kotlin" />
      <mapping ext="main.kts" type="Kotlin" />
      <mapping pattern="another test file" type="PLAIN_TEXT" />
      <mapping pattern="testfile" type="PLAIN_TEXT" />
      <mapping ext="bak" type="PLAIN_TEXT" />
      <mapping ext="exec" type="PLAIN_TEXT" />
      <mapping ext="jelly" type="XML" />
      <removed_mapping ext="bat" approved="true" type="PLAIN_TEXT" />
      <removed_mapping ext="cmd" approved="true" type="PLAIN_TEXT" />
      <removed_mapping ext="conf" approved="true" type="Logstash" />
      <removed_mapping ext="conf" approved="true" type="SmartyConfig" />
      <removed_mapping ext="dockerfile" approved="true" type="textmate" />
      <removed_mapping ext="jobdsl" approved="true" type="Groovy" />
      <removed_mapping ext="log" approved="true" type="PLAIN_TEXT" />
      <removed_mapping ext="makefile" approved="true" type="textmate" />
      <removed_mapping ext="snakefile" approved="true" type="textmate" />
      <removed_mapping pattern=".coveragerc" approved="true" type="PLAIN_TEXT" />
      <mapping pattern=".coveragerc" type="Ini" />
      <mapping pattern=".flake8" type="Ini" />
      <mapping pattern=".pydocstyle" type="Ini" />
      <mapping pattern=".pylintrc" type="Ini" />
    </extensionMap>
  </component>
</application>